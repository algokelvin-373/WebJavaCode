<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
	id="WebApp_ID" version="4.0">
	
  <display-name>HttpServletLifecycleAlgoKelvin</display-name>
  
  <welcome-file-list>
    <welcome-file>home</welcome-file>
  </welcome-file-list>
  
  <servlet>
	  <servlet-name>HttpServletLifecycle</servlet-name>
	  <servlet-class>com.algokelvin.servletlifecycle.HttpServletLifecycle</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HttpServletLifecycle</servlet-name>
  	<url-pattern>/myServletLifecycle</url-pattern>
  </servlet-mapping>
  
  <servlet>
	  <servlet-name>HomeServlet</servlet-name>
	  <servlet-class>com.algokelvin.servletlifecycle.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HomeServlet</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  
</web-app>