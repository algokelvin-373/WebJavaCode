<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
	id="WebApp_ID" version="4.0">
	
  <display-name>WebScopeSessionAlgoKelvin</display-name>
  
  <welcome-file-list>
    <welcome-file>web-scope-session</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<display-name>SessionScopeApp Page</display-name>
	<servlet-name>SessionScopeApp</servlet-name>
	<servlet-class>com.algokelvin.webscopesession.SessionScopeApp</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SessionScopeApp</servlet-name>
  	<url-pattern>/web-scope-session</url-pattern>
  </servlet-mapping>
  
</web-app>